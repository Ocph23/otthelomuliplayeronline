<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Game Othello</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/otthelo.css" />
    <link rel="stylesheet" href="~/css/board.css" />
    <link rel="stylesheet" href="~/lib/sweetalert2/dist/sweetalert2.css" />
  
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-page="/Index">Game Othello</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartial" />
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
                        </li>
                        
                       
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        @RenderBody()
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - MainWebGame - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

    <script src="~/lib/angular/angular.js"></script>
    <script src="~/lib/uirouter/dist/angular-ui-router.js"></script>
    <script src="~/lib/angular-local-storage/dist/angular-local-storage.js"></script>
    <script src="~/apps/gameLib/ai.lib.js"></script>
    <script src="~/apps/gameLib/chessboard1.lib.js"></script>
    <script src="~/apps/gameLib/othello.lib.js"></script>
    
    <script src="~/apps/app.js"></script>
    <script src="~/apps/routers/app.router.js"></script>
    <script src="~/apps/routers/account.router.js"></script>

    <script src="~/apps/services/app.services.js"></script>
    <script src="~/apps/services/auth.services.js"></script>
    <script src="~/apps/services/data.services.js"></script>
    <script src="~/apps/services/helper.services.js"></script>
    <script src="~/apps/services/message.services.js"></script>
    <script src="~/apps/services/storage.services.js"></script>

    <script src="~/apps/controllers/app.controller.js"></script>
    <script src="~/apps/controllers/account.controllers.js"></script>

    <script src="~/apps/components/app.component.js"></script>
    <script src="~/apps/components/useronline/app.useronline.component.js"></script>

    <script src="~/js/site.js" ></script>

    

<script src="~/lib/sweetalert2/dist/sweetalert2.js"></script>
<script src="~/lib/swangular/swangular.js"></script>
<script src="~/lib/microsoft/signalr/dist/browser/signalr.js"></script>

 @RenderSection("Scripts", required: false)
</body>
</html>
